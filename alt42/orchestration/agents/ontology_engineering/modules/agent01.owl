<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:at="http://mathking.kr/ontology/alphatutor#"
         xmlns:mk-a01-core="http://mathking.kr/ontology/agent01/core/"
         xmlns:mk-a01-task="http://mathking.kr/ontology/agent01/task/"
         xmlns:mk-a01-mod="http://mathking.kr/ontology/agent01/modules/"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#">

  <owl:Ontology rdf:about="http://mathking.kr/ontology/agent01/">
    <rdfs:label xml:lang="ko">Agent01 Onboarding Ontology</rdfs:label>
    <rdfs:comment xml:lang="ko">Agent01 온보딩 에이전트 전용 온톨로지</rdfs:comment>
    <owl:imports rdf:resource="core.owl"/>
  </owl:Ontology>

  <!-- ============================================ -->
  <!-- Agent Core Ontology -->
  <!-- ============================================ -->

  <!-- Agent Core 네임스페이스 속성 정의 (core.owl의 속성을 재사용하되, agent01 전용 네임스페이스로 확장) -->
  
  <!-- 기본 추상 클래스 -->
  <owl:Class rdf:about="http://mathking.kr/ontology/agent01/core/ContextBase">
    <rdfs:label xml:lang="ko">ContextBase</rdfs:label>
    <rdfs:comment xml:lang="ko">Agent01의 모든 Context 클래스가 상속받는 기본 추상 클래스</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://mathking.kr/ontology/alphatutor#Context"/>
  </owl:Class>

  <owl:Class rdf:about="http://mathking.kr/ontology/agent01/core/DecisionBase">
    <rdfs:label xml:lang="ko">DecisionBase</rdfs:label>
    <rdfs:comment xml:lang="ko">Agent01의 모든 Decision 클래스가 상속받는 기본 추상 클래스</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://mathking.kr/ontology/alphatutor#DecisionModel"/>
  </owl:Class>

  <owl:Class rdf:about="http://mathking.kr/ontology/agent01/core/ExecutionBase">
    <rdfs:label xml:lang="ko">ExecutionBase</rdfs:label>
    <rdfs:comment xml:lang="ko">Agent01의 모든 Execution 클래스가 상속받는 기본 추상 클래스</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://mathking.kr/ontology/alphatutor#ExecutionPlan"/>
  </owl:Class>

  <!-- 열거형 타입 정의 (owl:oneOf 사용) -->
  
  <!-- DifficultyLevel -->
  <owl:Class rdf:about="http://mathking.kr/ontology/agent01/core/DifficultyLevel">
    <rdfs:label xml:lang="ko">DifficultyLevel</rdfs:label>
    <rdfs:comment xml:lang="ko">수업/문제의 난이도를 표현하는 열거형 타입</rdfs:comment>
    <owl:oneOf rdf:parseType="Collection">
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/Easy">
        <rdfs:label xml:lang="ko">Easy</rdfs:label>
      </rdf:Description>
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/EasyToMedium">
        <rdfs:label xml:lang="ko">EasyToMedium</rdfs:label>
      </rdf:Description>
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/Medium">
        <rdfs:label xml:lang="ko">Medium</rdfs:label>
      </rdf:Description>
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/MediumToHard">
        <rdfs:label xml:lang="ko">MediumToHard</rdfs:label>
      </rdf:Description>
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/Hard">
        <rdfs:label xml:lang="ko">Hard</rdfs:label>
      </rdf:Description>
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/VeryHard">
        <rdfs:label xml:lang="ko">VeryHard</rdfs:label>
      </rdf:Description>
    </owl:oneOf>
  </owl:Class>

  <!-- AlignmentStrategy -->
  <owl:Class rdf:about="http://mathking.kr/ontology/agent01/core/AlignmentStrategy">
    <rdfs:label xml:lang="ko">AlignmentStrategy</rdfs:label>
    <rdfs:comment xml:lang="ko">학원-학교-집 진도를 정렬하는 전략을 표현하는 열거형 타입</rdfs:comment>
    <owl:oneOf rdf:parseType="Collection">
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/BridgeStrategy">
        <rdfs:label xml:lang="ko">BridgeStrategy</rdfs:label>
        <rdfs:comment xml:lang="ko">진도 간극 메우기 전략</rdfs:comment>
      </rdf:Description>
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/ReinforcementStrategy">
        <rdfs:label xml:lang="ko">ReinforcementStrategy</rdfs:label>
        <rdfs:comment xml:lang="ko">보강 전략</rdfs:comment>
      </rdf:Description>
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/PreviewStrategy">
        <rdfs:label xml:lang="ko">PreviewStrategy</rdfs:label>
        <rdfs:comment xml:lang="ko">예습 전략</rdfs:comment>
      </rdf:Description>
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/SynchronizedStrategy">
        <rdfs:label xml:lang="ko">SynchronizedStrategy</rdfs:label>
        <rdfs:comment xml:lang="ko">동기화 전략</rdfs:comment>
      </rdf:Description>
    </owl:oneOf>
  </owl:Class>

  <!-- ConfidenceLevel -->
  <owl:Class rdf:about="http://mathking.kr/ontology/agent01/core/ConfidenceLevel">
    <rdfs:label xml:lang="ko">ConfidenceLevel</rdfs:label>
    <rdfs:comment xml:lang="ko">학생의 수학 자신감 수준을 표현하는 열거형 타입</rdfs:comment>
    <owl:oneOf rdf:parseType="Collection">
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/VeryLow">
        <rdfs:label xml:lang="ko">VeryLow</rdfs:label>
      </rdf:Description>
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/Low">
        <rdfs:label xml:lang="ko">Low</rdfs:label>
      </rdf:Description>
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/Medium">
        <rdfs:label xml:lang="ko">Medium</rdfs:label>
      </rdf:Description>
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/High">
        <rdfs:label xml:lang="ko">High</rdfs:label>
      </rdf:Description>
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/VeryHigh">
        <rdfs:label xml:lang="ko">VeryHigh</rdfs:label>
      </rdf:Description>
    </owl:oneOf>
  </owl:Class>

  <!-- StressLevel -->
  <owl:Class rdf:about="http://mathking.kr/ontology/agent01/core/StressLevel">
    <rdfs:label xml:lang="ko">StressLevel</rdfs:label>
    <rdfs:comment xml:lang="ko">학생의 수학 스트레스 수준을 표현하는 열거형 타입</rdfs:comment>
    <owl:oneOf rdf:parseType="Collection">
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/StressLow">
        <rdfs:label xml:lang="ko">Low</rdfs:label>
      </rdf:Description>
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/StressMedium">
        <rdfs:label xml:lang="ko">Medium</rdfs:label>
      </rdf:Description>
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/StressHigh">
        <rdfs:label xml:lang="ko">High</rdfs:label>
      </rdf:Description>
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/StressVeryHigh">
        <rdfs:label xml:lang="ko">VeryHigh</rdfs:label>
      </rdf:Description>
    </owl:oneOf>
  </owl:Class>

  <!-- MathLevel -->
  <owl:Class rdf:about="http://mathking.kr/ontology/agent01/core/MathLevel">
    <rdfs:label xml:lang="ko">MathLevel</rdfs:label>
    <rdfs:comment xml:lang="ko">학생의 전반적인 수학 수준을 표현하는 열거형 타입</rdfs:comment>
    <owl:oneOf rdf:parseType="Collection">
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/Lower">
        <rdfs:label xml:lang="ko">Lower</rdfs:label>
      </rdf:Description>
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/LowerToMiddle">
        <rdfs:label xml:lang="ko">LowerToMiddle</rdfs:label>
      </rdf:Description>
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/Middle">
        <rdfs:label xml:lang="ko">Middle</rdfs:label>
      </rdf:Description>
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/MiddleToUpper">
        <rdfs:label xml:lang="ko">MiddleToUpper</rdfs:label>
      </rdf:Description>
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/Upper">
        <rdfs:label xml:lang="ko">Upper</rdfs:label>
      </rdf:Description>
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/Top">
        <rdfs:label xml:lang="ko">Top</rdfs:label>
      </rdf:Description>
    </owl:oneOf>
  </owl:Class>

  <!-- MathLearningStyle -->
  <owl:Class rdf:about="http://mathking.kr/ontology/agent01/core/MathLearningStyle">
    <rdfs:label xml:lang="ko">MathLearningStyle</rdfs:label>
    <rdfs:comment xml:lang="ko">학생의 수학 학습 스타일을 표현하는 열거형 타입</rdfs:comment>
    <owl:oneOf rdf:parseType="Collection">
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/CalculationType">
        <rdfs:label xml:lang="ko">CalculationType</rdfs:label>
        <rdfs:comment xml:lang="ko">계산형 - 계산과 연습 문제 중심</rdfs:comment>
      </rdf:Description>
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/ConceptType">
        <rdfs:label xml:lang="ko">ConceptType</rdfs:label>
        <rdfs:comment xml:lang="ko">개념형 - 개념 이해 중심</rdfs:comment>
      </rdf:Description>
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/ApplicationType">
        <rdfs:label xml:lang="ko">ApplicationType</rdfs:label>
        <rdfs:comment xml:lang="ko">응용형 - 응용 문제 중심</rdfs:comment>
      </rdf:Description>
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/MixedType">
        <rdfs:label xml:lang="ko">MixedType</rdfs:label>
        <rdfs:comment xml:lang="ko">혼합형 - 여러 스타일 혼합</rdfs:comment>
      </rdf:Description>
    </owl:oneOf>
  </owl:Class>

  <!-- ExamStyle -->
  <owl:Class rdf:about="http://mathking.kr/ontology/agent01/core/ExamStyle">
    <rdfs:label xml:lang="ko">ExamStyle</rdfs:label>
    <rdfs:comment xml:lang="ko">학생의 시험 대비 성향을 표현하는 열거형 타입</rdfs:comment>
    <owl:oneOf rdf:parseType="Collection">
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/Cramming">
        <rdfs:label xml:lang="ko">Cramming</rdfs:label>
        <rdfs:comment xml:lang="ko">벼락치기 - 시험 직전 집중 학습</rdfs:comment>
      </rdf:Description>
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/Planned">
        <rdfs:label xml:lang="ko">Planned</rdfs:label>
        <rdfs:comment xml:lang="ko">계획형 - 일정에 맞춘 계획적 학습</rdfs:comment>
      </rdf:Description>
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/Continuous">
        <rdfs:label xml:lang="ko">Continuous</rdfs:label>
        <rdfs:comment xml:lang="ko">지속형 - 꾸준한 학습</rdfs:comment>
      </rdf:Description>
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/Irregular">
        <rdfs:label xml:lang="ko">Irregular</rdfs:label>
        <rdfs:comment xml:lang="ko">불규칙형 - 불규칙한 학습 패턴</rdfs:comment>
      </rdf:Description>
    </owl:oneOf>
  </owl:Class>

  <!-- SeverityLevel -->
  <owl:Class rdf:about="http://mathking.kr/ontology/agent01/core/SeverityLevel">
    <rdfs:label xml:lang="ko">SeverityLevel</rdfs:label>
    <rdfs:comment xml:lang="ko">문제의 심각도를 표현하는 열거형 타입</rdfs:comment>
    <owl:oneOf rdf:parseType="Collection">
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/SeverityLow">
        <rdfs:label xml:lang="ko">Low</rdfs:label>
        <rdfs:comment xml:lang="ko">낮음 - 낮은 우선순위</rdfs:comment>
      </rdf:Description>
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/SeverityMedium">
        <rdfs:label xml:lang="ko">Medium</rdfs:label>
        <rdfs:comment xml:lang="ko">보통 - 중간 우선순위</rdfs:comment>
      </rdf:Description>
      <rdf:Description rdf:about="http://mathking.kr/ontology/agent01/core/SeverityHigh">
        <rdfs:label xml:lang="ko">High</rdfs:label>
        <rdfs:comment xml:lang="ko">높음 - 높은 우선순위</rdfs:comment>
      </rdf:Description>
    </owl:oneOf>
  </owl:Class>

  <!-- ============================================ -->
  <!-- Task Core Ontology -->
  <!-- ============================================ -->

  <owl:Class rdf:about="http://mathking.kr/ontology/agent01/task/OnboardingContextBase">
    <rdfs:label xml:lang="ko">OnboardingContextBase</rdfs:label>
    <rdfs:comment xml:lang="ko">온보딩 Task의 모든 Context Module이 상속받는 기본 클래스</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://mathking.kr/ontology/agent01/core/ContextBase"/>
  </owl:Class>

  <owl:Class rdf:about="http://mathking.kr/ontology/agent01/task/OnboardingDecisionBase">
    <rdfs:label xml:lang="ko">OnboardingDecisionBase</rdfs:label>
    <rdfs:comment xml:lang="ko">온보딩 Task의 모든 Decision Module이 상속받는 기본 클래스</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://mathking.kr/ontology/agent01/core/DecisionBase"/>
  </owl:Class>

  <owl:Class rdf:about="http://mathking.kr/ontology/agent01/task/OnboardingExecutionBase">
    <rdfs:label xml:lang="ko">OnboardingExecutionBase</rdfs:label>
    <rdfs:comment xml:lang="ko">온보딩 Task의 모든 Execution Module이 상속받는 기본 클래스</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://mathking.kr/ontology/agent01/core/ExecutionBase"/>
  </owl:Class>

  <!-- ============================================ -->
  <!-- Task Module Ontology -->
  <!-- ============================================ -->

  <!-- CandidateProblem: 문제 후보 구조체 -->
  <owl:Class rdf:about="http://mathking.kr/ontology/agent01/modules/CandidateProblem">
    <rdfs:label xml:lang="ko">CandidateProblem</rdfs:label>
    <rdfs:comment xml:lang="ko">Interpretation Layer에서 도출된 문제 후보 구조체</rdfs:comment>
  </owl:Class>

  <!-- CandidateProblem 속성 정의 -->
  <owl:DatatypeProperty rdf:about="http://mathking.kr/ontology/agent01/modules/hasProblemId">
    <rdfs:label xml:lang="ko">hasProblemId</rdfs:label>
    <rdfs:comment xml:lang="ko">문제 ID (예: "P1")</rdfs:comment>
    <rdfs:domain rdf:resource="http://mathking.kr/ontology/agent01/modules/CandidateProblem"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://mathking.kr/ontology/agent01/modules/hasDescription">
    <rdfs:label xml:lang="ko">hasDescription</rdfs:label>
    <rdfs:comment xml:lang="ko">문제 설명</rdfs:comment>
    <rdfs:domain rdf:resource="http://mathking.kr/ontology/agent01/modules/CandidateProblem"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:ObjectProperty rdf:about="http://mathking.kr/ontology/agent01/modules/hasSeverity">
    <rdfs:label xml:lang="ko">hasSeverity</rdfs:label>
    <rdfs:comment xml:lang="ko">심각도 (high/medium/low)</rdfs:comment>
    <rdfs:domain rdf:resource="http://mathking.kr/ontology/agent01/modules/CandidateProblem"/>
    <rdfs:range rdf:resource="http://mathking.kr/ontology/agent01/core/SeverityLevel"/>
  </owl:ObjectProperty>

  <owl:DatatypeProperty rdf:about="http://mathking.kr/ontology/agent01/modules/hasWillAlignment">
    <rdfs:label xml:lang="ko">hasWillAlignment</rdfs:label>
    <rdfs:comment xml:lang="ko">관련 Will 항목 리스트</rdfs:comment>
    <rdfs:domain rdf:resource="http://mathking.kr/ontology/agent01/modules/CandidateProblem"/>
    <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://mathking.kr/ontology/agent01/modules/hasDataSources">
    <rdfs:label xml:lang="ko">hasDataSources</rdfs:label>
    <rdfs:comment xml:lang="ko">데이터 소스 리스트</rdfs:comment>
    <rdfs:domain rdf:resource="http://mathking.kr/ontology/agent01/modules/CandidateProblem"/>
    <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
  </owl:DatatypeProperty>

  <!-- CandidateProblem 제약조건 -->
  <owl:Class rdf:about="http://mathking.kr/ontology/agent01/modules/CandidateProblem">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://mathking.kr/ontology/agent01/modules/hasProblemId"/>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://mathking.kr/ontology/agent01/modules/hasDescription"/>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://mathking.kr/ontology/agent01/modules/hasSeverity"/>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://mathking.kr/ontology/agent01/modules/hasDataSources"/>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <!-- OnboardingContext -->
  <owl:Class rdf:about="http://mathking.kr/ontology/agent01/modules/OnboardingContext">
    <rdfs:label xml:lang="ko">OnboardingContext</rdfs:label>
    <rdfs:comment xml:lang="ko">학생의 온보딩 정보와 초기 학습 맥락을 표현하는 데이터 구조</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://mathking.kr/ontology/agent01/task/OnboardingContextBase"/>
    <!-- Stage 제약조건: hasStage는 반드시 "Context"여야 함 -->
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://mathking.kr/ontology/alphatutor#hasStage"/>
        <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Context</owl:hasValue>
      </owl:Restriction>
    </rdfs:subClassOf>
    <!-- 필수 속성 제약조건 -->
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://mathking.kr/ontology/agent01/modules/hasStudentGrade"/>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://mathking.kr/ontology/agent01/modules/hasSchool"/>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://mathking.kr/ontology/agent01/modules/hasAcademy"/>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://mathking.kr/ontology/alphatutor#hasStage"/>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <!-- hasMathConfidence 범위 제약 (0-10) -->
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://mathking.kr/ontology/agent01/modules/hasMathConfidence"/>
        <owl:someValuesFrom>
          <rdfs:Datatype>
            <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
            <owl:withRestrictions rdf:parseType="Collection">
              <rdf:Description>
