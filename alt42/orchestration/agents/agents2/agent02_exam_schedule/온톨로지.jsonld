      "rdfs:subClassOf": "mk:Strategy"
    },
    {
      "@id": "mk:UnitPriority",
      "@type": "owl:Class",
      "rdfs:label": "단원 우선순위",
      "rdfs:comment": "단원별 우선순위",
      "rdfs:subClassOf": "mk:Strategy"
    },
    {
      "@id": "mk:TextbookStrategy",
      "@type": "owl:Class",
      "rdfs:label": "교재 전략",
      "rdfs:comment": "학원 교재별 활용 전략",
      "rdfs:subClassOf": "mk:Strategy"
    },
    {
      "@id": "mk:RoutineSchedule",
      "@type": "owl:Class",
      "rdfs:label": "루틴 일정",
      "rdfs:comment": "단기/중기 루틴 일정",
      "rdfs:subClassOf": "mk:Strategy"
    },
    {
      "@id": "mk:ErrorPattern",
      "@type": "owl:Class",
      "rdfs:label": "실수 패턴",
      "rdfs:comment": "반복 실수 패턴 (계산/개념 비율)",
      "rdfs:subClassOf": "mk:Context"
    },
    {
      "@id": "mk:ImprovementStrategy",
      "@type": "owl:Class",
      "rdfs:label": "개선 전략",
      "rdfs:comment": "다음 시험 루프 초기화 전략",
      "rdfs:subClassOf": "mk:Strategy"
    },
    {
